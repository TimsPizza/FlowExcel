{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:default",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "log:default",
    "shell:allow-execute",
    "shell:allow-spawn",
    "shell:allow-open",
    "opener:default",
    "opener:allow-reveal-item-in-dir"
  ]
}