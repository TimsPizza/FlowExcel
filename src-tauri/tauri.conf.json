{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "FlowExcel",
  "version": "0.1.0",
  "identifier": "com.timspizza.flowexcel",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "pnpm start",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "FlowExcel",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "center": true,
        "visible": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "binaries/flowexcel-backend/**/*"
    ]
  },
  "plugins": {
    "shell": {
      "open": true
    }
  }
}
